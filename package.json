{
  "name": "learning-orchestrator",
  "version": "2.1.1",
  "description": "ODI Learning Orchestrator: connects HubSpot, Forecast, Moodle, and Google Calendar for bookings, enrollments, and scheduling.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon index.js",
    "debug": "EMAIL_DEBUG=true nodemon index.js",
    "debug:test": "node test-debug-mode.js",
    "debug:test:simple": "node debug-test-simple.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:config": "node tests/test-config.js",
    "test:projects": "node tests/test-project-dropdown.js",
    "test:email-lookup": "node tests/test-email-lookup.js",
    "test:restful-api": "node tests/test-restful-api.js",
    "test:task-linking": "node tests/test-task-linking.js",
    "test:calendar-setup": "node tests/test-google-calendar-setup.js",
    "test:all": "npm run test:config && npm run test:projects && npm run test:email-lookup && npm run test:restful-api",
    "test:run": "node tests/run-tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/theodi/learning-orchestrator.git"
  },
  "author": "David Tarrant",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theodi/learning-orchestrator/issues"
  },
  "homepage": "https://github.com/theodi/learning-orchestrator#readme",
  "dependencies": {
    "ajv": "^8.6.1",
    "axios": "^1.8.4",
    "csurf": "^1.11.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.2",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.0",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "googleapis": "^128.0.0",
    "method-override": "^3.0.0",
    "mongoose": "^8.18.0",
    "nodemailer": "^7.0.6",
    "nodemon": "^3.1.9",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0"
  }
}
